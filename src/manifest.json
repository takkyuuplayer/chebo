{
  "name": "extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome extension bootstarp.",
  "content_scripts": [
        {
            "permissions": [
                "http://*.google.co.jp/",
                "https://*.google.co.jp/"
            ],
            "matches": [
                "http://*.google.co.jp/*",
                "https://*.google.co.jp/*"
            ],
            "js": [
                "./vendor/underscore-1.4.4.min.js",
                "./vendor/jquery-1.9.1.min.js",
                "./vendor/backbone-1.0.0.min.js",
                "./vendor/backbone.localStorage-1.1.0.min.js",
                "./js/foreground.js"
                ],
            "run_at": "document_end"
        }
  ]
}
